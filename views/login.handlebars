<div class="row">
  <form class="login-form col s6 offset-s3">
    <div>
      <label for="email-login">Email:</label>

      <input type="text" id="email-login" /> 

    </div>
    <div>
      <label for="password-login">Password:</label>
      <input type="password" id="password-login" />
    </div>
    <div>
      <button type="submit" id="login-submit" class="waves-effect waves-light btn">Login</button>
    </div>
    <div>
        <label for="signup-redirect">Not a member?</label>
        </br>

        <button type="submit" id="signup-redirect" class="waves-effect waves-light btn">Sign up!</button>
    </div>
  </form> 
</div>
<script src="/javascript/login.js"></script>